@model VkazoMaterial.Models.Rank
@using VkazoMaterial.Resources

@{
    ViewBag.Title = "Löschen";
}

<h2>Delete</h2>

<h3>@AppResource.ConfirmDelete</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.RankName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RankName)
        </dd>

